  <xs:attributeGroup name="tei_att.measurement.attributes">
    <xs:attributeGroup ref="tei:tei_att.measurement.attribute.unit"/>
    <xs:attributeGroup ref="tei:tei_att.measurement.attribute.unitRef"/>
